            <h4 class="line-divider">Contact</h4>
            <h1>Say hello!</h1>
            <div class="row">
                <div class="span8 offset2">
                    <p>
                        We like to create things with fun, open-minded people. Feel free to say hello! We'll respond as soon as possible.
                    </p>
                </div>
            </div>
        </header>
        <!-- End Header -->
        
        <!-- Start Article -->
        <article>
            <div class="row-fluid">
                <!-- Contact form -->
                <div class="span6">
                    <h5>Drop Us A Line</h5>
                      <%= form_for(@message, url: email_deliver_path) do |f|%>
                        <%= f.text_field(:name, class: 'span12', placeholder: 'Name', required: true )%>
                        <%= f.text_field(:from, class: 'span12', placeholder: 'Email', required: true )%>
                        <%= f.text_area(:body, class: 'span12', placeholder: 'Some Words On How We Can Help...', required: true, rows: '10')%>
                        <%= f.hidden_field(:direction, value: 'incoming') %>
                        <%= f.hidden_field(:subject, value: 'New Contact') %>
                        <%= f.hidden_field(:kind, value: 'Home Page Contact Form')%>
                        <%= f.submit('Submit', class: 'btn btn-primary') %>
                      <% end %>
                </div>
                <!-- End Contact form -->
                <!-- Contact information -->
                <div class="span5 offset1">
                    <h5>Meet Us</h5>
                    <div class="media">
                        <div class="social-icons sicon-white pull-left">
                            <a href="#" class="sicon-phone"><i>Phone</i></a>
                        </div> 
                        <div class="media-body smallFontBy08">
                            (+1) 702 418-4771 <em>Jeff Bradshaw</em><br/>
                            (+1) 702 245-8569 <em>Tim Lowe</em>
                        </div>
                    </div>
                    <div class="media">
                        <div class="social-icons sicon-white pull-left">
                            <a href="#" class="sicon-place"><i>Location</i></a>
                        </div> 
                        <div class="media-body smallFontBy08">
                            9525 Hillwood Dr Ste 120, Las Vegas, NV 89134
                        </div>
                    </div>
                    <div class="media">
                        <div class="social-icons sicon-white pull-left">
                            <a href="#" class="sicon-mail"><i>email</i></a>
                        </div> 
                        <div class="media-body smallFontBy08">
                            bradshawsellinglv@gmail.com<br/>
                            timlowsellsvegas@gmail.com
                        </div>
                    </div>
                    <div class="media">
                        <div class="social-icons sicon-white pull-left">
                            <a href="#" class="sicon-clock"><i>Time zone</i></a>
                        </div> 
                        <div class="media-body smallFontBy08">
                            UTC / PST <%= Time.now.strftime("%I:%M%pico") %>
                        </div>
                    </div>
                    <br/>
                    <br/>
                    <br/>
                    <h5>Get social</h5>
                    <div class="social-icons sicon-white bordered">
                        <a href="#" class="sicon-facebook"><i>Facebook</i></a>
                        <a href="#" class="sicon-twitter"><i>Twitter</i></a>
                        <a href="#" class="sicon-linkedin"><i>LinkedIn</i></a>
                        <a href="#" class="sicon-youtube"><i>Youtube</i></a>
                        <a href="#" class="sicon-pinterest"><i>Pinterest</i></a>
                    </div>
                </div>
                <!-- End Contact information -->
            </div>
        </article>
        <!-- End Article -->
        
    </div>
    
    <!-- Background map image -->
    <div id="map-image">
        <img src="assets/pages/contact/map_bck.jpg" alt="Sudden Impact Media">
    </div>
</div>
<!-- END PAGE | CONTACT -->

<script>
	flowplayer("player", "<%= asset_path('flowplayer/flowplayer-3.2.18.swf') %>", {
	  clip: {
	    autoPlay: false,
      autoBuffering: true,
	  }
	});
</script>